generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model projects {
  id          Int       @id @default(autoincrement())
  name        String    @unique
  description String
  startPeriod DateTime
  endPeriod   DateTime?
  createdAt   DateTime  @default(now())
  team        String
}
